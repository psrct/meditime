<%- include("head") %>

  <body>
    <%-include("sidebar")%>
    <%-include("main-content-top")%>
    <h1>Table Of Task</h1>
    <table>
      <thead>
        <tr>
          <th>TaskID</th>
          <th>PatientID</th>
          <th>Start</th>
          <th>End</th>
          <th>Completed?</th>
        </tr>
      </thead>

      <tbody>
        <% data.forEach((item)=> { %>
          <tr>
            <td>
              <%= item.task_id %>
            </td>
            <td>
              <%= item.patient_id %>
            </td>
            <td>
              <%= item.start_datetime %>
            </td>
            <td>
              <%= item.end_datetime %>
            </td>
            <td>
              <%= item.is_completed ? "✅" : "❌" %>
            </td>
          </tr>
          <% }) %>
      </tbody>
    </table>
    <%-include("main-content-bottom")%>
    <%-include("script-toggle-sidebar")%>
  </body>

  </html>